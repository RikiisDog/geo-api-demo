{
    "editor.tabSize": 4,
    "eslint.enable": true,
    "eslint.format.enable": false,
    "eslint.validate": ["javascript", "javascriptreact"],
    "eslint.run": "onSave",
    "css.lint.vendorPrefix": "ignore",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true
    },
    "[javascript]": {
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
    }
}
